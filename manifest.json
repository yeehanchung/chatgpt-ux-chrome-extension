{
    "manifest_version": 3,
    "name": "chatgpt-ux",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "service_worker": "content-script.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}
